    "nodecg",
    "vue"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint-config-xo-typescript": "^0.32.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodecg": "^1.0.0",
    "typescript": "^4.0.3",
    "vue": "^3.0.0",
    "xo": "^0.33.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "git add"
  },
  "xo": {
    "prettier": true
  }
}
