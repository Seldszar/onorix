{
  "name": "onorix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Alexandre Breteau <contact@seldszar.fr> (https://seldszar.fr)",
  "description": "Yet another NodeCG plugin for Vue",
  "repository": "seldszar/onorix",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
